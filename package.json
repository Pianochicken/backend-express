{
  "name": "backend-express",
  "version": "1.0.0",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsoa spec-and-routes && tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop -d src/app-data-source.ts",
    "seed": "ts-node ./src/scripts/seedAdmin.ts",
    "seed:prod": "node ./dist/scripts/seedAdmin.js",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate ./src/migrations/InitSchema -d src/app-data-source.ts",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/app-data-source.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/app-data-source.ts",
    "migration:run:prod": "node ./node_modules/typeorm/cli.js migration:run -d dist/app-data-source.js",
    "migration:revert:prod": "node ./node_modules/typeorm/cli.js migration:revert -d dist/app-data-source.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.19.1",
    "@types/pg": "^8.15.6",
    "@types/swagger-ui-express": "^4.1.8",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "nyc": "^17.1.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.6.0",
    "typeorm": "^0.3.28",
    "winston": "^3.18.3"
  }
}
